<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.xy.roomBean.RoomInfo" table="tb_room_info">
		<id name="id" column="id"></id>
		<property name="roomNum" column="roomNum"></property>
		<property name="price" column="price"></property>
		<property name="phone" column="phone"></property>
		<property name="care" column="care"></property>
		<property name="startTime" column="startTime"></property>
		<property name="endTime" column="endTime"></property>
		<property name="days" column="days"></property>
		<property name="state" column="state"></property>
		<property name="createTime" column="createTime"></property>
	</class>
</hibernate-mapping>