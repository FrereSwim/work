<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.xy.memberBean.MemberInfo" table="tb_member_info">
		<id name="id" column="id"></id>
		<property name="mName" column="mName"></property>
		<property name="mPhone" column="mPhone"></property>
		<property name="mCare" column="mCare"></property>
		<property name="mBirth" column="mBirth"></property>
		<property name="mType" column="mType"></property>
		<property name="createTime" column="createTime"></property>
	</class>
</hibernate-mapping>