<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<!-- 1 配置类和表对应 
		class标签
		name属性：实体类全路径
		table属性：数据库表名称
	-->
	<class name="cn.xy.bean.Partner" table="tb_partner_info">
		<id name="id" column="id"></id>
		<property name="partnerName" column="partnerName"></property>
		<property name="money" column="money"></property>
		<property name="type" column="type"></property>
		<property name="cooperationTime" column="cooperationTime"></property>
		<property name="startTime" column="startTime"></property>
	</class>
</hibernate-mapping>