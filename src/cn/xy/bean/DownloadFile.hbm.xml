<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.xy.bean.DownloadFile" table="tb_download_info">
		<id name="id" column="id"></id>
		<property name="fileName" column="fileName"></property>
		<property name="uploadName" column="uploadName"></property>
		<property name="showName" column="showName"></property>
		<property name="fileType" column="fileType"></property>
		<property name="createTime" column="createTime"></property>
	</class>
</hibernate-mapping>