<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.xy.menuBean.DishBillInfo" table="tb_dishbill_info">
		<id name="id" column="id"></id>
		<property name="tableNum" column="tableNum"></property>
		<property name="price" column="price"></property>
		<property name="createTime" column="createTime"></property>
		<property name="actId" column="actId"></property>
		<property name="actName" column="actName"></property>
		<property name="mid" column="mid"></property>
	</class>
</hibernate-mapping>