<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.xy.warehouseBean.GoodsInfo" table="tb_goods_info">
		<id name="id" column="id"></id>
		<property name="goodsName" column="goodsName"></property>
		<property name="type" column="type"></property>
		<property name="num" column="num"></property>
		<property name="unit" column="unit"></property>
		<property name="unitPrice" column="unitPrice"></property>
		<property name="warden" column="warden"></property>
		<property name="createTime" column="createTime"></property>
	</class>
</hibernate-mapping>